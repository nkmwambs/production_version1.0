<script>


	$('#chkAll').click(function(){
		$(this).toggleClass('selected','not-selected');
		
		if($(this).hasClass('selected')){
			$('.chk_voucher').prop('checked',true);
					var cnt = 0;
					$('.chk_voucher').each(function(){
						if($(this).is(':checked')){
							++cnt;
						}
					});
			$(this).html(cnt+' Vouchers Selected');		
		}else{
			$('.chk_voucher').prop('checked',false);
			$(this).html('Select Vouchers');
		}
		
		count_checked_boxes();
	});
	
	$('.chk_voucher').click(function(){
		count_checked_boxes();
		
	});

	function count_checked_boxes(){
		var cnt_checked = 0;
		$('.chk_voucher').each(function(){
			if($(this).is(':checked')){
				++cnt_checked;
			}
		});
		
		if(cnt_checked>0){
			$('#print_vouchers').css('display','block');
			$("#chkAll").html(cnt_checked+' Vouchers Selected');	
		}else{
			$('#print_vouchers').css('display','none');	
			$("#chkAll").html('Select Vouchers');
		}
	}

function format ( d ) {
	var obj = JSON.parse(d);
	var cost="";
	var account="";
	var sum = 0;
	
	$.each(obj,function(index,value){
		cost += "<td>"+accounting.formatMoney(value.Cost, { symbol: "",  format: "%v %s" })+"</td>";
		account += "<th>"+value.AccNo+"</th>";
		sum = parseFloat(sum) + parseFloat(value.Cost);
	});
	
	return 	"<table border='1' style='border-collapse:collapse;'>"+
			"<thead>"+
			"<tr>"+account+"<th>Total</th></tr>"+
			"</thead>"+
			"<tbody>"+
			"<tr>"+cost+"<td>"+accounting.formatMoney(sum, { symbol: "",  format: "%v %s" })+"</td></tr>"+
			"</tbody>"+
			"</table>";
}

	$(document).ready(function(){
		var datatable = $('.datatable').DataTable({
			lengthMenu: [[25,50, 100, 150,-1], [25,50 ,100,150 ,"All"]],
			pageLength: 25,
			dom: '<"row"l><Bf><"col-sm-12"rt><ip>',
		    pagingType: "full_numbers",
		    ordering:false,	
		    buttons: [
		         'csv', 'excel', 'print'
		    ],	      
		    stateSave: true,
		    oLanguage: {
			        sProcessing: "<img src='https://www.compassionkenya.com/tools/uploads/preloader4.gif'>"
			    },
			processing: true, //Feature control the processing indicator.
			"columnDefs": [
			       	{ 
			           "targets": [0], //first column / numbering column
			           "orderable": false, //set not orderable
			           "class": "details-control"
			       	}
			  ],
			  fixedHeader: true    	
		});
		
		datatable.columns( '.spread' ).visible( false );
		
		$("#toggleColumns").click(function(ev){
			
			$(this).toggleClass('btn-success','btn-warning');
			
			if($(this).hasClass('btn-success')){
				datatable.columns( '.spread' ).visible( true );
				$(this).html('Hide Spread');

			}else{
				datatable.columns( '.spread' ).visible( false );
				$(this).html('Show Spread');

			}
			
		});
		
		var detailRows = [];
		
		  $('#table_export tbody').on( 'click', 'tr td.details-control', function () {
	        var tr = $(this).closest('tr');
	        var row = datatable.row( tr );
	        var td_id = $(this).attr('id');
	        var idx = $.inArray( tr.attr('id'), detailRows );
	 		//alert(td_id);
	        if ( row.child.isShown() ) {
	            tr.removeClass( 'details' );
	            row.child.hide();
	            tr.removeClass('shown')
	 
	            // Remove from the 'open' array
	            detailRows.splice( idx, 1 );
	            
	            //Change fa-plus to fa-minus
	            //row.child.hide();
	        }
	        else {
	        	//alert(td_id);
	            
	            	
	            	$.ajax({
						url:'https://www.compassionkenya.com/tools/ifms.php/partner/get_ajax_voucher/'+td_id,
						success:function(data){
							
							tr.addClass( 'details' );
							row.child( format( data )).show();
				 			tr.addClass('shown');
				            // Add to the 'open' array
				            if ( idx === -1 ) {
				                detailRows.push( tr.attr('id') );
				            }
				            
						}
					});
	            
	        }
	    } );
	 
	    // On each draw, loop over the `detailRows` array and show any child rows
	    datatable.on( 'draw', function () {
	        $.each( detailRows, function ( i, id ) {
	            $('#'+id+' td.details-control').trigger( 'click' );
	        } );
	    } );
	});
	
    function PrintElem(elem)
    {
        $(elem).printThis({ 
		    debug: false,              
		    importCSS: true,             
		    importStyle: true,         
		    printContainer: false,       
		    loadCSS: "", 
		    pageTitle: "Payment Voucher",             
		    removeInline: false,        
		    printDelay: 333,            
		    header: null,             
		    formValues: true          
		});
    }	
    
    //Clear records
	$('.clr').on('switch-change', function(e, data) {
			var el = $(this);
	       	var hID = el.attr('id');
			//alert(data.value);
			
		        var state = 1;
		        if(data.value==true){
		        	state = 0;
		        }
		        
		        	var url = 'https://www.compassionkenya.com/tools/ifms.php/partner/clear_bank_transactions/'+hID+'/'+state+'/2020-03-31';	
					$.ajax({
							url: url
						});	
	        
	   });
	   
$('.scroll').click(function(ev){
	
	var cnt = $('#cnt').val();
	
	if(cnt===""){
		cnt = "1";
	}
	
	var dt = '1585612800';
	
	var flag = $(this).attr('id');
	
	var url = 'https://www.compassionkenya.com/tools/ifms.php/facilitator/scroll_cash_journal/KE397/'+dt+'/'+cnt+'/'+flag;
	
	$(this).attr('href',url);
});


$('#print_vouchers').click(function(){
	//alert("Under Construction");
	
	$('#cj_print_vouchers').submit();
	
});

function delete_all_vouchers(project,hID){
	var result = window.confirm('Are You Sure You Want To Delete This Voucher And Above?');
        
            if(!result) {
		                 BootstrapDialog.show({
		                 	title:'Information',
				            message: 'Process Aborted'
				        });
	            }else{
					var url = 'https://www.compassionkenya.com/tools/ifms.php/facilitator/delete_all_vouchers/'+project+'/'+hID+'/1585612800';
					
					$.ajax({
						url:url,
						success:function(response){
							BootstrapDialog.show({
								title:'Alert',
								message:response
							});
							
							location.reload();
						}
						
					});
				}	
}

function delete_voucher(project,hID){
	
			var result = window.confirm('Are You Sure You Want To Delete This Voucher?');
        
            if(!result) {
		                 BootstrapDialog.show({
		                 	title:'Information',
				            message: 'Process Aborted'
				        });
	            }else{
					var url = 'https://www.compassionkenya.com/tools/ifms.php/facilitator/delete_voucher/'+project+'/'+hID+'/1585612800';
					
					$.ajax({
						url:url,
						success:function(response){
							BootstrapDialog.show({
								title:'Alert',
								message:response
							});
							
							location.reload();
						}
					});
				}	
				
}

function remove_duplicate(project,hID){
	
			var result = window.confirm('Are You Sure You Want To Delete This Voucher?');
        
            if(!result) {
		                 BootstrapDialog.show({
		                 	title:'Information',
				            message: 'Process Aborted'
				        });
	            }else{
					var url = 'https://www.compassionkenya.com/tools/ifms.php/facilitator/remove_duplicate/'+project+'/'+hID+'/1585612800';
					
					$.ajax({
						url:url,
						success:function(response){
							BootstrapDialog.show({
								title:'Alert',
								message:response
							});
							
							location.reload();
						}
					});
				}	
				
}
